{
  "expo": {
    "name": "Navigation",
    "slug": "navigation",
    "scheme": "navigation",
    "version": "1.0.0",
    "orientation": "default",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.webridge.navigation"
    },
    "androidStatusBar": {
      "translucent": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      },
      "package": "com.webridge.navigation"
    },
    "web": {
      "favicon": "./assets/favicon.png",
      "name": "Navigation",
      "description": "Navigation"
    },
    "newArchEnabled": true,
    "plugins": [
      "./expo-plugin-android-material-you-bottom-bar"
    ]
  }
}
